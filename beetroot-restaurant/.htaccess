
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /beetroot_bistro/beetroot-restaurant/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /beetroot_bistro/beetroot-restaurant/index.php [L]
</IfModule>

# END WordPress